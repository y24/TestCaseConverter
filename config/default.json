{
  "output_format": "markdown",
  "split_mode": "per_sheet",
  "id_prefix": "TC",
  "id_padding": 3,
  "id_start_number": 1,
  "force_id_regenerate": false,
  "sheet_search_keys": [
    "テスト項目"
  ],
  "sheet_search_ignores": [],
  "header": {
    "search_col": "A",
    "search_key": "#"
  },
  "category_row": {
    "keys": [
      "大項目",
      "中項目",
      "小項目1",
      "小項目2"
    ],
    "ignores": []
  },
  "step_row": {
    "keys": [
      "手順",
      "実施手順"
    ],
    "ignores": []
  },
  "tobe_row": {
    "keys": [
      "期待結果"
    ],
    "ignores": []
  },
  "test_type_row": {
    "keys": [
      "テスト種別"
    ],
    "ignores": []
  },
  "priority_row": {
    "keys": [
      "優先度"
    ],
    "ignores": []
  },
  "precondition_row": {
    "keys": [
      "前提条件"
    ],
    "ignores": []
  },
  "note_row": {
    "keys": [
      "備考",
      "補足情報"
    ],
    "ignores": []
  },
  "backlog_id_cell": {
    "keys": [
      "案件チケットID"
    ],
    "ignores": []
  },
  "test_type_cell": {
    "keys": [
      "テスト種別",
      "テストフェーズ"
    ],
    "ignores": []
  },
  "test_target_cell": {
    "keys": [
      "テスト対象(機能/モジュール)",
      "対象モジュール"
    ],
    "ignores": []
  },
  "target_version_cell": {
    "keys": [
      "テスト対象バージョン",
      "対象バージョン"
    ],
    "ignores": []
  },
  "test_environments_cell": {
    "keys": [],
    "ignores": ["テスト環境", "テスト環境情報"]
  },
  "trim_whitespaces": false,
  "normalize_zenkaku_alphanumeric": true,
  "normalize_step_numbers": true,
  "category_display_compress": false,
  "pad_category_levels": true,
  "forward_fill_category": true,
  "single_line_fields": {
    "category_row": true,
    "title_row": true,
    "test_type_row": true,
    "priority_row": true
  },
  "convert_linebreaks_to_spaces": true,
  "output_source_info": true,
  "output_meta_info": true
}